{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base", "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-magic-numbers": "error",
        "no-console": "off",
        "import/extensions": "off",
        "radix": "off"
    }
}
